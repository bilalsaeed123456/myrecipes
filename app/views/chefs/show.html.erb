<%= render 'shared/page_title', title: "Chef #{@chef.chefname}'s profile" %>
<% if @chef.recipes.count != 0%>
    <h4 align="center"><%= @chef.chefname %>'s recipes</h4>
    <div class="center">
        <%= will_paginate @chef_recipes %>
    </div>
    <%= render 'recipes/recipe', obj: @chef_recipes %>
    <div class="center">
        <%= will_paginate @chef_recipes %>
    </div>
    <div class="col-md-4 col-md-offset-4 colormatch">
        [ <%= link_to "Create your recipe", new_recipe_path %> ]
    </div>
<% else %>
    <h4 align="center">No Recipes Yet!</h4>
<% end %>
